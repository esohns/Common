# libCommonConfig.cmake.in
#  LIBCOMMON_INCLUDE_DIRS - include directories for FooBar
#  LIBCOMMON_LIBRARIES    - libraries to link against

# Compute paths
get_filename_component (LIBCOMMON_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set (LIBCOMMON_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# library dependencies (contains definitions for IMPORTED targets)
if (NOT TARGET libCommon AND NOT LIBCOMMON_BINARY_DIR)
include ("${LIBCOMMON_CMAKE_DIR}/CommonTargets.cmake")
endif ()

# These are IMPORTED targets created by libCommonTargets.cmake
set (LIBCOMMON_LIBRARIES libCommon)
