                <property name="fill">False</property>
                <property name="position">0</property>
              </packing>
            </child>
            <child>
              <widget class="GtkDrawingArea" id="drawingarea">
                <property name="visible">True</property>
                <signal name="expose_event" handler="drawingarea_expose_event_cb"/>
                <signal name="key_press_event" handler="drawingarea_key_press_event_cb"/>
                <signal name="configure_event" handler="drawingarea_configure_event_cb"/>
              </widget>
              <packing>
                <property name="position">1</property>
              </packing>
            </child>
            <child>
              <widget class="GtkHBox" id="hbox1">
                <property name="visible">True</property>
                <child>
                  <widget class="GtkProgressBar" id="progressbar">
                    <property name="visible">True</property>
